{
  "methods": ["GET", "POST"],
  "uri": "/oms/*",
  "upstream": {
    "type": "roundrobin",
    "nodes": {
      "oms-api:8000": 1
    }
  },
  "plugins": {
    "proxy-rewrite": {
      "regex_uri": ["^/oms/(.*)", "/$1"]
    },
    "cors": {
      "allow_origins": "*",
      "allow_methods": "GET,POST,PUT,DELETE,OPTIONS",
      "allow_headers": "*"
    }
  },
  "desc": "OMS API routes - correlates outage events from multiple sources"
}

