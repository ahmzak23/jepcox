{
  "uri": "/hes-mock-generator",
  "name": "hes-mock-generator",
  "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "HEAD", "OPTIONS", "CONNECT", "TRACE"],
  "upstream": {
    "type": "roundrobin",
    "scheme": "http",
    "nodes": {
      "web2": 1
    },
    "timeout": {
      "connect": 6,
      "send": 6,
      "read": 6
    },
    "pass_host": "pass",
    "keepalive_pool": {
      "size": 320,
      "requests": 1000,
      "idle_timeout": 60
    }
  }
}

